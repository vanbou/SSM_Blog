<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tulun.dao.CategoryMapper">
    <select id="getAllCategory" resultType="com.tulun.model.Category">
        select * from t_category
    </select>

    <select id="getCategoryById" resultType="com.tulun.model.Category">
        select * from t_category where id = #{id}
    </select>
</mapper>